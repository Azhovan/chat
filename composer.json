{
  "name": "azhovan/chat",
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "squizlabs/php_codesniffer": "3.*"
  },
  "require": {
    "php": ">=7.2",
    "mockery/mockery": "^1.2",
    "ext-json": "*",
    "ext-openssl": "*",
    "illuminate/database": "^6.10",
    "illuminate/events": "^6.10",
    "vlucas/phpdotenv": "^4.1",
    "nesbot/carbon": "^2.28",
    "illuminate/filesystem": "^6.11",
    "illuminate/http": "6.11.*",
    "illuminate/routing": "^6.11",
    "league/fractal": "^0.18.0",
    "illuminate/validation": "^6.11",
    "guzzlehttp/guzzle": "^6.5"
  },
  "autoload": {
    "psr-4": {
      "Chat\\": "src/"
    }
  },
  "autoload-dev": {
    "Tests\\": "tests/"
  },
  "scripts": {
    "tests": "vendor/bin/phpunit",
    "run" : " $(which php) -S localhost:8000",
    "setup" : "$(which php) setup.php",
    "build" : " $(which composer) dump-autoload -o"
  }
}
